{
    "name": "miladtech/laravel-support",
    "description": "MiladTech common support helpers, contracts, and traits required by various MiladTech packages. Validator functionality, and basic controller included out-of-the-box.",
    "type": "library",
    "keywords": [
        "miladtech",
        "laravel",
        "contract",
        "validator",
        "timezones",
        "mimetype",
        "support",
        "helper",
        "trait"
    ],
    "license": "MIT",
    "homepage": "https://milad-tech.com/",
    "support": {
        "email": "info@milad-tech.com",
        "issues": "https://github.com/mrmostafaei/laravel-support/issues",
        "source": "https://github.com/mrmostafaei/laravel-support",
        "docs": "https://github.com/mrmostafaei/laravel-support/blob/master/README.md"
    },
    "authors": [
        {
            "name": "MiladTech LLC",
            "homepage": "https://milad-tech.com",
            "email": "info@milad-tech.com"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/mrmostafaei/laravel-support/contributors"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/auth": "^8.0.0 || ^9.0.0 || ^10.0.0",
        "illuminate/bus": "^8.0.0 || ^9.0.0 || ^10.0.0",
        "illuminate/contracts": "^8.0.0 || ^9.0.0 || ^10.0.0",
        "illuminate/http": "^8.0.0 || ^9.0.0 || ^10.0.0",
        "illuminate/routing": "^8.0.0 || ^9.0.0 || ^10.0.0",
        "illuminate/support": "^8.0.0 || ^9.0.0 || ^10.0.0",
        "illuminate/validation": "^8.0.0 || ^9.0.0 || ^10.0.0",
        "watson/validating": "^7.0.0 || ^8.0.0",
        "spatie/laravel-schemaless-attributes": "^2.0.0 || ^2.4.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.31.0",
        "phpunit/phpunit": "^9.5.0 || ^10.0"
    },
    "autoload": {
        "files": [
            "src/Support/helpers.php"
        ],
        "psr-4": {
            "MiladTech\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MiladTech\\Support\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MiladTech\\Support\\Providers\\SupportServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
